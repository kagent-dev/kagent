          "spec": {
            "containers": [
              {
                "args": [
                  "--host",
                  "0.0.0.0",
                  "--port",
                  "8080",
                  "--filepath",
                  "/config"
                ],
                "env": [
                  {
                    "name": "OPENAI_API_KEY",
